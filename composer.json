{
    "name": "hydrawiki/custom-mw-fixers",
    "description": "A set of custom fixers for PHP CS Fixer and the Hydra project",
    "type": "library",
    "keywords": [
        "fixer",
        "php-cs-fixer",
        "hydra"
    ],
    "license": "GNU GENERAL PUBLIC LICENSE",
    "authors": [
        {
            "name": "Samuel Hilson",
            "email": "shhilson@curse.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "ext-tokenizer": "*",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "sebastian/diff": "^3.0",
        "squizlabs/php_codesniffer": "^3.2",
        "symfony/console": "^4.1",
        "symfony/finder": "^4.1",
        "symfony/yaml": "^4.1",
        "vimeo/psalm": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "CustomMWFixers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}